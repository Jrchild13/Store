{
  "Rocklib.Logging": {
    "Providers": [
      {
        "Type": "RockLib.Logging.ConsoleLogProvider, RockLib.Logging",
        "Value": {
          "Template": "{level}: {message}",
          "Level": "Info",
          "Output": "StdOut",
          "Timeout": "00:00:01"
        }
      }
    ]
  }
}
